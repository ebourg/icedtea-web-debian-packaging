#!/bin/sh

set -e

basedir=/@basedir@
base7dir=/@base7dir@
tools='javaws'

if [ "$1" = "remove" ] || [ "$1" = "deconfigure" ]; then
    for i in $tools; do
        update-alternatives --remove $i $basedir/jre/bin/$i
    done
    for i in $tools; do
        update-alternatives --remove $i $base7dir/jre/bin/$i
    done
fi

